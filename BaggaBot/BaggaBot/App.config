<?xml version="1.0" encoding="utf-8"?>
<configuration>
  
  <configSections>
    <section name="InactivityTimerSection" type="BaggaBot.InactivityConfiguration, BaggaBot" />
  </configSections>
  
  <InactivityTimerSection InactivityPeriodMinutes="1440" IntervalSeconds="60" DisplayMessage="@here Its been 24 hours since a post .. c'mon all! ">
    <Channels>
      <Channel Name="" Id="" />
    </Channels>
  </InactivityTimerSection>
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  
  <appSettings>
    <add key="bot-token" value="" />
  </appSettings>

  <runtime>

    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">

      <dependentAssembly>

        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />

        <bindingRedirect oldVersion="0.0.0.0-8.0.0.0" newVersion="8.0.0.0" />

      </dependentAssembly>

    </assemblyBinding>

  </runtime>
</configuration>